
<%- include('header') -%>
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>


<div class="box">

  <% let i ; %>
 <% for(i =0; i < newItems.length; i++ ) { %>
   <div class="item">
     <input type="checkbox" >
     <p> <%= newItems[i] %> </p>
   </div>
   <% } %>

    <form  action="/" method="post" class="item">
      <input type="text" name="listItem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="list" value= <%= listTitle %> >+</button>
    </form>

  </div>

<%- include('footer') -%>
